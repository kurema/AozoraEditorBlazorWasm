@inject Blazored.LocalStorage.ILocalStorageService localStorage
@page "/"

<CascadingValue Name="AppModel" Value="@Model">
	<FullEditor />
</CascadingValue>

@code {
	Models.ApplicationModel? Model { get; set; }

	protected override void OnInitialized()
	{
		Model = new Models.ApplicationModel(localStorage);
		base.OnInitialized();
	}
}
